{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "Compilacion personalizada",
			"command": "/usr/bin/g++",
			"args": [
				"-o", "main",
                "main.cpp",
				"Vector.cpp"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build",
			"detail": "compilador: /usr/bin/g++"
		}
	]
}